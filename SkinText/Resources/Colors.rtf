                                <Thumb x:Name="ToolBarThumb" Style="\{StaticResource ToolBarThumbStyle\}" Width="10" />\par
                                <ToolBarPanel x:Name="PART_ToolBarPanel" IsItemsHost="true" Margin="0,1,2,2" />\par
                            </DockPanel>\par
                        </Border>\par
                        <ControlTemplate.Triggers>\par
                            <Trigger Property="IsOverflowOpen" Value="true">\par
                                <Setter TargetName="ToolBarThumb" Property="IsEnabled" Value="false" />\par
                            </Trigger>\par
                            <Trigger Property="ToolBarTray.IsLocked" Value="true">\par
                                <Setter TargetName="ToolBarThumb" Property="Visibility" Value="Collapsed" />\par
                            </Trigger>\par
                        </ControlTemplate.Triggers>\par
                    </ControlTemplate>\par
                </Setter.Value>\par
            </Setter>\par
        </Style>\par
\par
        <Style x:Key="\{x:Type ToolBarTray\}" TargetType="\{x:Type ToolBarTray\}">\par
            <Setter Property="Background" Value="#aa535353">\par
            </Setter>\par
        </Style>\par
